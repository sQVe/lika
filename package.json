    "lint:ci:eslint": "eslint '{src,test}/**/*.ts'",
    "lint:ci:prettier": "prettier --check '{**/*,*}.{json,md,ts,yaml}'",
    "lint:eslint": "eslint --fix '{src,test}/**/*.ts'",
    "lint:prettier": "prettier --write '{**/*,*}.{json,md,ts,yaml}'",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-sqve": "^3.0.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
